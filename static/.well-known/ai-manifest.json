{
  "name": "devcycle-apis",
  "version": "0.5.0",
  "apis": [
    {
      "name": "bucketing-api",
      "main": "https://docs.devcycle.com/redocusaurus/bucketing-api.yaml",
      "auth": {
        "type": "none"
      }
    },
    {
      "name": "management-api",
      "main": "https://api.devcycle.com/swagger.json",
      "auth": {
        "type": "oauth",
        "client_url": "https://auth.devcycle.com/oauth/token",
        "authorization_url": "https://auth.devcycle.com/oauth/token",
        "authorization_content_type": "application/x-www-form-urlencoded",
        "verification_tokens": {
          "openai": "Replace_this_string_with_the_verification_token_generated_in_the_ChatGPT_UI"
        }
      }
    }
  ],
  "description": "A plugin that provides easy access to the APIs."
}
